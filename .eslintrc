{
  "extends": ["airbnb"],
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "react/jsx-filename-extension": 0,
    "comma-dangle": 0,
    "arrow-parens": 0,
    "react/jsx-props-no-spreading": 0,
    "implicit-arrow-linebreak": 0,
    "react/no-unused-state" : 0,
    "react/forbid-prop-types": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "parser": "babel-eslint"
}
